<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor
        configVersion="3"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="https://www.phpdoc.org"
>
    <title>phpDocumentor</title>
    <paths>
        <output>build/docs</output>
    </paths>
    <version number="3.0">
        <api format="php">
        <source dsn=".">
            <path>.</path>
        </source>
        <ignore hidden="true" symlinks="true">
            <path>.php-cs-fixer.dist.php</path>
            <path>templates/src/Entity</path>
            <path>html</path>
            <path>vendor</path>
        </ignore>
        <extensions>
            <extension>php</extension>
        </extensions>
        </api>
    </version>
</phpdocumentor>
