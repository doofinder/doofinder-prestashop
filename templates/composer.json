{
  "name": "prestashop/doofinder",
  "version": "${PLUGIN_VERSION}",
  "description": "Doofinder module for PrestaShop",
  "require-dev": {
    "prestashop/php-dev-tools": "^5",
    "friendsofphp/php-cs-fixer": "^3.0",
    "phpdocumentor/shim": "^3.8"
  },
  "autoload": {
    "psr-4": {
      "PrestaShop\\Module\\Doofinder\\": "src/"
    },
    "classmap": [
      "doofinder.php"
    ]
  },
  "config": {
    "prepend-autoloader": false,
    "lock": false,
    "allow-plugins": {
      "phpdocumentor/shim": true
    }
  },
  "type": "prestashop-module"
}
